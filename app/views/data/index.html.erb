<h1>Listing data</h1>

<%= link_to "Get New Tinder Users", data_get_users_from_tinder_path %>
<br>
<%= link_to "Delete all Users", data_delete_all_users_path %>

<table>
  <thead>
  <tr>
    <th>Number of User</th>
    <th>Number of Photos</th>
  </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @users.count %></td>
        <td><%= @photos.count %></td>
      </tr>
  </tbody>
</table>



<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= link_to 'Show', datum %></td>
        <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
        <td><%= link_to 'Destroy', datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datum', new_datum_path %>
